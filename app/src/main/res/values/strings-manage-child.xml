<resources>
    <string name="manage_child_tab_categories">Categories</string>
    <string name="manage_child_tab_other">Other</string>
    <string name="manage_child_block_temporarily_title">block temporarily</string>
    <string name="manage_child_block_temporarily_text">
        Select categories which should be blocked temporarily.
        Temporarily does not mean that the blocking is disabled automatically.
        If you need a recurring blocking, use blocked time areas.
    </string>

    <string name="manage_child_category_no_time_limits">no time limit</string>

    <string name="manage_child_categories_intro_title">Categories</string>
    <string name="manage_child_categories_intro_text">
        TimeLimit manages the limits using categories.
        You can select multiple Apps and limits per category.
        This allows for example to set one limit for all games.
    </string>

    <string name="manage_child_manipulation_title" translatable="false">@string/manage_device_manipulation_title</string>
    <string name="manage_child_manipulation_text">
        There was a manipulation.
        Go back to the overview and open the device entry for details.
    </string>
</resources>
